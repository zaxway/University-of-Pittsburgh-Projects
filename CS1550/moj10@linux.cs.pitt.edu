void consoleintr(int (*getc)(void))
{
  acquire(&graphics.lock);
  while((c = getc()) >= 0){
      if(c != 0 && input.e-input.r < INPUT_BUF){
        input.buf[input.e++ % INPUT_BUF] = c;
      }
  }
  release(&graphics.lock);
}

int blit() {
	unsigned char * fb = (unsigned char *) P2V(0xa0000);
	for(int p=0; p<4; p++){
		set_plane(p);
		memmove(fb, offscreen_buffer[p], 640*640/8);
	}
}

int draw_pixel(int x, int y, int color) {
	int byteNum[8] = offscreen_buffer[0][y*640/8+x/8]; // each one these returns a byte
	int index = x%8; // use this to take a certain bit out of each byte
	int bitNum = byteNum[index]; // This is our byte either a 1 or a 0
	unsigned int mask = 0x80 >> bitNum;

	int bit[4]; // These are the bits of the color
	bit[0] = color & 0x01; // the is the first bit of the color
	if (bit[0] == 0) {
		offscreen_buffer[0][y*640/8+x/8] |= mask;
	}
	else {
		offscreen_buffer[0][y*640/8+x/8] &= ~mask;
	}
	bit[1] = color & 0x02; // second bit
	if (bit[1] == 0) {
		offscreen_buffer[0][y*640/8+x/8] |= mask;
	}
	else {
		offscreen_buffer[0][y*640/8+x/8] &= ~mask;
	}
	bit[2] = color & 0x04; // third bit
	if (bit[2] == 0) {
		offscreen_buffer[0][y*640/8+x/8] |= mask;
	}
	else {
		offscreen_buffer[0][y*640/8+x/8] &= ~mask;
	}
	bit[3] = color & 0x08; // fourth bit
	if (bit[3] == 0) {
		offscreen_buffer[0][y*640/8+x/8] |= mask;
	}
	else {
		offscreen_buffer[0][y*640/8+x/8] &= ~mask;
	}

	// second byte
	int byteNum2[8] = offscreen_buffer[1][y*640/8+x/8];
	index = x%8; // use this to take a certain bit out of each byte
	bitNum = byteNum[index]; // This is our byte either a 1 or a 0
	mask = 0x80 >> bitNum;

	if (bit[0] == 0) {
		offscreen_buffer[1][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[1][y*640/8+x/8] & ~mask;
	}
	if (bit[1] == 0) {
		offscreen_buffer[1][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[1][y*640/8+x/8] & ~mask;
	}
	if (bit[2] == 0) {
		offscreen_buffer[1][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[1][y*640/8+x/8] & ~mask;
	}
	if (bit[3] == 0) {
		offscreen_buffer[1][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[1][y*640/8+x/8] & ~mask;
	}

	// Third Byte
	int byteNum2[8] = offscreen_buffer[2][y*640/8+x/8];
	index = x%8; // use this to take a certain bit out of each byte
	bitNum = byteNum[index]; // This is our byte either a 1 or a 0
	mask = 0x80 >> bitNum;

	if (bit[0] == 0) {
		offscreen_buffer[2][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[2][y*640/8+x/8] & ~mask;
	}
	if (bit[1] == 0) {
		offscreen_buffer[2][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[2][y*640/8+x/8] & ~mask;
	}
	if (bit[2] == 0) {
		offscreen_buffer[2][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[2][y*640/8+x/8] & ~mask;
	}
	if (bit[3] == 0) {
		offscreen_buffer[2][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[2][y*640/8+x/8] & ~mask;
	}

	// fourth byte
	int byteNum2[8] = offscreen_buffer[3][y*640/8+x/8];
	index = x%8; // use this to take a certain bit out of each byte
	bitNum = byteNum[index]; // This is our byte either a 1 or a 0
	mask = 0x80 >> bitNum;

	if (bit[0] == 0) {
		offscreen_buffer[3][y*640/8+x/8] | mask;
	}
	else {
		offscreen_buffer[3][y*640/8+x/8] & ~mask;
	}
