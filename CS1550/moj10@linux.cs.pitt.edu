	int dx = x2 - x1;
	int dy = y2 - y1;
	int form = 2*dy - dx;
	int y = y1;
	int i;
	for(i = 0; i < dx; i++) {
		draw_pixel(i,y,15);
		if form > 0 {
			y = y + 1;
			from = form - 2*dx
