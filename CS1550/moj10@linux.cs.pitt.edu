}

int sys_getkey(void)
{
  int c;
  if(input.r == input.e)
        return -1;

  c = input.buf[input.r++ % INPUT_BUF];
  return c;
}

int
sys_exit_graphics(void)
{

	write_regs(g_80x25_text);
	write_font(g_8x16_font, 16);
        graphics_mode = 0;
	clear_screen();
	return 0;
}


int clear_screen() {
	int x = 0;
	int y = 0;
	for (x = 0; x < 640; x++) {
		for (y = 0; y < 480; y++) {
			draw_pixel(x,y,0);
		}
	}
	blit();
}
